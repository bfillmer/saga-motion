  "private": true,
  "dependencies": {
    "@curi/core": "1.0.0-beta.25",
    "@curi/redux": "^1.0.0-beta.2",
    "@hickory/browser": "^1.0.0-beta.5",
    "axios": "0.17.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "^5.0.6",
